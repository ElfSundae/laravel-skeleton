{
    "name": ":vendor/:package_name",
    "type": "library",
    "description": "",
    "keywords": [":package_name"],
    "homepage": "https://github.com/:github_username/:package_name",
    "license": "MIT",
    "authors": [
        {
            "name": ":author_name",
            "email": ":author_email",
            "homepage": ":author_website"
        }
    ],
    "require": {
        "php": "~7.1",
        "illuminate/support": "~5.0|~6.0|~7.0|~8.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~5.7|~6.0|~7.0|~8.0|~9.0"
    },
    "autoload": {
        "psr-4": {
            ":psr4_namespace\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            ":psr4_namespace\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                ":psr4_namespace\\:package_serviceServiceProvider"
            ],
            "aliases": {
                ":package_service": ":psr4_namespace\\Facades\\:package_service"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
