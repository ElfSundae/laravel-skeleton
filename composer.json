{
    "name": ":vendor/:package_name",
    "type": "library",
    "homepage": "https://github.com/:vendor/:package_name",
    "license": "MIT",
    "description": "",
    "keywords": [
        ":vendor",
        ":package_name",
        "laravel"
    ],
    "authors": [
        {
            "name": ":author_name",
            "email": ":author_email",
            "homepage": ":author_website"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/console": "5.*",
        "illuminate/support": "5.*"
    },
    "require-dev": {
        "mockery/mockery": "~0.9.4",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            ":namespace\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            ":namespace\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
